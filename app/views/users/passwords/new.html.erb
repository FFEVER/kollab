<div class="authen">
  <h2>Welcome to</h2>
  <div class="mt-3">
    <%= image_pack_tag 'kollab-blue.png', alt: 'kollab', size:'150x70'%>
  </div>
  <div class="mt-3">
    <h1>Forgot Password</h1>
    <div class="d-flex flex-column">
      <%= react_component("authen/ForgotPassword",
    {
      authenticityToken: form_authenticity_token,
      signUpPath: new_user_registration_path
    })
  %>

    </div>

    <div class="d-flex flex-row align-items-center mt-3" >
      <p class="mr-2">Don't have an account?</p>
      <a class="link" href="http://localhost:5000/users/sign_up">Sign Up</a>
    </div>
  </div>
</div>

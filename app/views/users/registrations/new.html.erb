<div class="authen">
  <h2>Welcome to</h2>
  <div class="mt-3">
    <%= image_pack_tag 'kollab-blue.png', alt: 'kollab', size:'150x70'%>
  </div>
  <div class="mt-3">
    <h1>Sign Up</h1>
    <div class="d-flex flex-column">
      <%= react_component("authen/SignUp",
    {
      authenticityToken: form_authenticity_token,
      submitPath: user_registration_path
    })
  %>

    </div>
    <div class="d-flex flex-row align-items-center mt-3" >
      <p class="mr-2">Already have an account?</p>
      <%= link_to "Sign in", new_user_session_path, class: "link" %>
    </div>
  </div>
</div>
